{
  "name": "philsquare/laramanager",
  "description": "Manager for Laravel applications",
  "keywords": ["laravel","admin"],
  "homepage": "https://github.com/philsquare/LaraManager",
  "license": "MIT",
  "authors": [
    {
      "name": "Philsquare LLC",
      "email": "hello@philsquare.com",
      "homepage": "http://philsquare.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Philsquare\\LaraManager\\": "src/Philsquare/LaraManager"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "intervention/image": "~2.2",
    "intervention/imagecache": "~2.3",
    "suin/php-rss-writer": "^1.4",
    "doctrine/dbal": "^2.5"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Philsquare\\LaraManager\\Providers\\LaraManagerServiceProvider"
      ]
    }
  }
}
